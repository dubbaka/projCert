Vagrant.configure("2") do |config|
    #config.ssh.private_key_path = "/home/nath/.vagrant.d/insecure_private_key"
  
  config.vm.define "server{{ build }}" do |server{{ build }}|
    config.vm.provider "virtualbox" do |w{{ build }}|
      w{{ build }}.memory = 1500
    end
    server{{ build }}.vm.box = "bento/ubuntu-18.04"
    server{{ build }}.vm.network "public_network", bridge: "enp5s0", ip: "192.168.1.{{ build }}"
    server{{ build }}.vm.network "public_network", bridge: "wlp3s0", type: "dhcp"
    server{{ build }}.vm.network "private_network", ip: "192.168.56.{{ build }}"
  end

end
